{
  "name": "creatina-design",
  "version": "1.0.1",
  "description": "Sitio web Creatina Design",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "global:package": "node-sass pug pug-cli browser-sync parallelshell rimraf mkdirp uncss postcss-cli autoprefixer buildify useref useref-cli html-minifier copy imagemin imagemin-cli imagemin-pngquant imagemin-jpeg-recompress imagemin-webp imagemin-svgo imagemin-gifsicle",
    "dev:package": "node-sass pug pug-cli browser-sync parallelshell rimraf mkdirp uncss postcss-cli autoprefixer buildify useref useref-cli html-minifier copy imagemin imagemin-cli imagemin-pngquant imagemin-jpeg-recompress imagemin-webp imagemin-svgo imagemin-gifsicle",
    "sass": "node-sass -w -o ./src/css ./src/sass",
    "pug": "pug -w -P -p ./src/pug/partials -O ./src/pug/options.json ./src/pug/templates -o ./src",
    "watch:metalang": "parallelshell \"npm run sass\" \"npm run pug\"",
    "dev:server": "browser-sync start --server --startPath ./src --serverStatic ./src --files ./src --reload-delay=300 --reload-debounce=500",
    "watch:devserver": "parallelshell \"npm run dev:server\" \"npm run watch:metalang\"",
    "clean": "rimraf ./dist/*",
    "build:folders": "mkdirp ./dist/css ./dist/js ./dist/img",
    "uncssFiles": "uncss \"./src/**/*.+(html|php)\" > ./dist/css/local_styles.css",
    "autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers \" > 5%, ie 10\" -r ./dist/css/style.css",
    "min:css-js": "node buildify"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.4.7",
    "browser-sync": "^2.26.3",
    "buildify": "^0.4.0",
    "copy": "^0.3.2",
    "html-minifier": "^3.5.21",
    "imagemin": "^6.1.0",
    "imagemin-cli": "^4.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^5.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "parallelshell": "^3.0.2",
    "postcss-cli": "^6.1.1",
    "pug": "^3.0.1",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.3",
    "uncss": "^0.16.2",
    "useref": "^1.4.1",
    "useref-cli": "^1.0.0"
  }
}
