#portfolio.container-fluid.py-5
  .container
    #gallery
      .row
        .col-6.col-md.m-0.p-0
          .flex-container
            .flex-cell
              .flex-item.print
                a.portfolio(href="#print" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="print")
                  h1.h2.text-uppercase.text-center.portfolio-section.text-shadow printed
